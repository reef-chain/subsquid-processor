{
  "name": "squid-reef",
  "version": "2.1.2",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "update": "npx npm-check-updates --filter /subsquid/ --upgrade && npm i -f",
    "db:migrate": "npx squid-typeorm-migration apply",
    "processor:start": "node lib/processor.js",
    "query-node:start": "squid-graphql-server",
    "lint": "eslint . --ext .ts --ignore-path .gitignore --ignore-pattern src/abi/**/*  --fix"
  },
  "dependencies": {
    "@subsquid/archive-registry": "^3.3.0",
    "@subsquid/big-decimal": "^1.0.0",
    "@subsquid/cli": "^2.13.0",
    "@subsquid/graphql-server": "^4.5.0",
    "@subsquid/ss58": "^2.0.1",
    "@subsquid/substrate-processor": "^7.2.1",
    "@subsquid/typeorm-migration": "^1.2.2",
    "@subsquid/typeorm-store": "^1.2.4",
    "dotenv": "^16.0.0",
    "ethers": "^6.8.1",
    "firebase-admin": "^12.0.0",
    "graphql-type-json": "^0.3.2",
    "ipfs-http-client": "56.0.2",
    "jsonwebtoken": "^9.0.0",
    "mqtt": "^5.3.5",
    "pg": "^8.7.3",
    "pusher": "^5.2.0",
    "type-graphql": "^1.2.0-rc.1",
    "typeorm": "^0.3.6",
    "@types/lodash": "^4.17.5"
